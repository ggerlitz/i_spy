<h3 class="formTitle">Log in!</h3>

<%= form_tag "/sessions", method: :post do %>
    <p>
        <%= label_tag :username %>
        <%= text_field_tag :username %>
    </p>
    <p>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
    </p>
    <p>
        <%= submit_tag "log in" %>
    </p>

<% end %>