<%= form_for @post, html: { multipart: true } do |f| %>
	<p><%= f.label :title, "Upload an Image" %>
		<%= f.file_field :avatar %>
	</p>
  <p><%= f.label :title %>
      <%= f.text_field :title %>
  </p>

  <p><%= f.label :body %>
      <%= f.text_area :body %>
  </p>

  <p><%= f.submit %></p>
<% end %>